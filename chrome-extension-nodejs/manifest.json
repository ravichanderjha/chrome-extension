{
  "name": "Page Redder",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Turns the page red when you click the icon",
  "permissions": [
    "activeTab", 
    "webNavigation",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["data.js"]
    }
  ]
}